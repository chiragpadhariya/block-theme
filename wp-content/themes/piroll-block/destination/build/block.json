        {
          "heading": "Slide 2",
          "content": "Content for Slide 2"
        },
        {
          "heading": "Slide 3",
          "content": "Content for Slide 3"
        }
      ]
    }
  },
  "textdomain": "destination",
  "editorScript": "file:./index.js",
  "editorStyle": "file:./index.css",
  "style": "file:./style-index.css",
  "render": "file:./render.php"
}